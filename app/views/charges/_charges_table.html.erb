<h1><%= title %></h1>
<table>
    <thead>
        <tr>
            <th>Customer Name</th>
            <th>Amount</th>
            <th>Disputed Date</th>
        <tr>
    </thead>
    <tbody>
        <% charges.each do |charge| %>
        <tr>
            <td><%= charge.customer.full_name %></td>
            <td><%= charge.amount %></td>
            <td><%= charge.timestamp_to_date %></td>
        </tr>
        <% end %>
    </tbody>
</table>